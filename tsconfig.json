{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@models/*": [
        "models/*"
      ],
      "@data/*": [
        "data/*"
      ],
      "@layouts/*": [
        "layouts/*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@utils/*": [
        "utils/*"
      ],
      "@icons/*": [
        "icons/*"
      ],
      "@sections/*": [
        "sections/*"
      ],
      "@layouts": [
        "layouts/*"
      ],
    }
  }
}